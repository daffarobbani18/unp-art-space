{
  "github": {
    "silent": false
  },
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/build/web/(.*)",
      "destination": "/build/web/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/build/web/$1"
    },
    {
      "source": "/((?!.*\\.).*)",
      "destination": "/build/web/index.html"
    }
  ]
}